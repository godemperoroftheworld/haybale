{
  "schemaVersion": 1,
  "id": "haybale",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "Library mod for Tenzin's mods!",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/tlib",
    "sources": "https://github.com/t2pellet/tlib"
  },
  "license": "AGPLv3",
  "icon": "assets/haybale/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.t2pellet.tlib.TenzinLibFabric"
    ],
    "client": [
      "com.t2pellet.tlib.TenzinLibFabric"
    ],
    "modmenu": [ "com.t2pellet.tlib.client.compat.FabricModMenuCompat"]
  },
  "mixins": [
    "haybale.mixins.json",
    "haybale.fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.13",
    "fabric": ">=0.54.0",
    "minecraft": "1.19.x",
    "java": ">=17"
  },
  "suggests": {
    "modmenu": "*",
    "cloth-config": "*"
  }
}
  